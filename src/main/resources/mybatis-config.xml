<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="kr.ac.kopo.exhibition.model.Member" alias="Member"/>
		<typeAlias type="kr.ac.kopo.exhibition.model.Event" alias="Event"/>
		<typeAlias type="kr.ac.kopo.exhibition.model.Photo" alias="Photo"/>
		<typeAlias type="kr.ac.kopo.exhibition.model.Price" alias="Price"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/member.xml"/>
		<mapper resource="mapper/event.xml"/>
	</mappers>
</configuration>
